{
  "name": "@sumup/sumup-ts-codegen",
  "version": "0.0.1",
  "description": "OpenAPI client generator",
  "keywords": [
    "sdk",
    "generator",
    "openapi"
  ],
  "author": "Matous Dzivjak",
  "license": "Apache-2.0",
  "homepage": "https://github.com/sumup/sumup-ts",
  "bugs": {
    "url": "https://github.com/sumup/sumup-ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sumup/sumup-ts.git"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "provenance": true
  },
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "bin": {
    "sumup-ts-codegen": "dist/index.cjs"
  },
  "tsup": {
    "clean": true,
    "entry": [
      "src/index.ts"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "shims": true
  },
  "scripts": {
    "build": "tsup-node",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "prepublishOnly": "npm run build",
    "tsc": "tsc"
  },
  "dependencies": {
    "@biomejs/js-api": "^3.0.0",
    "@biomejs/wasm-nodejs": "^2.2.6",
    "change-case-all": "^2.1.0",
    "commander": "^14.0.0",
    "swagger-parser": "^10.0.3",
    "ts-pattern": "^5.7.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "@types/minimist": "^1.2.5",
    "@types/node": "^24.1.0",
    "openapi-types": "^12.1.3",
    "tsafe": "^1.6.6",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
